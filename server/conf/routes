# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                             controllers.Application.index
GET        /Events                       controllers.Application.index
GET        /AboutTED                     controllers.Application.aboutTED
GET        /Sponsors                     controllers.Application.sponsors
GET        /Contact						 controllers.Application.contact
GET       /fie93krubvla92s94dkwjf9dk/:auth/:title/:subtitle/:speaker/:desc/:venue/:date/:time/:seats/:link             controllers.Application.submitEventForm(auth:String,title:String,subtitle:String,speaker:String,desc:String,venue:String,date:String,time:String,seats:String,link:String)
#POST       /fie93krubvla92s94dkwjf9dk    controllers.Application.submitEventForm

GET        /Test                         controllers.Test.index
# Prefix must match `play.assets.urlPrefix`

GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
